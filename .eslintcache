ge={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n        <FormField>\r\n          <input\r\n            type='text'\r\n            placeholders='Category'\r\n            name='category'\r\n            value={values.category}\r\n            onChange={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n        <FormField>\r\n          <input\r\n            type='text'\r\n            placeholders='Description'\r\n            name='description'\r\n            value={values.description}\r\n            onChange={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n        <FormField>\r\n          <input\r\n            type='text'\r\n            placeholders='City'\r\n            name='city'\r\n            value={values.city}\r\n            onChange={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n        <FormField>\r\n          <input\r\n            type='text'\r\n            placeholders='Venue'\r\n            name='venue'\r\n            value={values.venue}\r\n            onChange={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n        <FormField>\r\n          <input\r\n            type='date'\r\n            placeholders='Date'\r\n            name='date'\r\n            value={values.date}\r\n            onChange={(e) => handleInputChange(e)}\r\n          />\r\n        </FormField>\r\n\r\n        <Button type='submit' floated='right' positive content='Submit' />\r\n        <Button\r\n          onClick={() => setFormOpen(false)}\r\n          type='submit'\r\n          floated='right'\r\n          content='Cancel'\r\n        />\r\n      </Form>\r\n    </Segment>\r\n  );\r\n}\r\n","C:\\Users\\Clover\\Documents\\Udemy\\Build an app with React, Redux, Firestore from Scratch - Neil\\revents\\src\\app\\api\\sampleData.js",[],"C:\\Users\\Clover\\Documents\\Udemy\\Build an app with React, Redux, Firestore from Scratch - Neil\\revents\\src\\app\\store\\configureStore